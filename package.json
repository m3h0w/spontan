{
  "name": "spontan",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --fix --ext .ts,.tsx",
    "fix": "organize-imports-cli tsconfig.json && prettier --config .prettierrc --write **/*.{ts,tsx} && eslint --fix",
    "vercel:publish": "expo build:web && cp -r .vercel ./web-build/.vercel && cd web-build && vercel && cd ..",
    "pub": "concurrently 'expo publish' 'yarn vercel:publish'",
    "postinstall": "bash ./fix-for-web.sh"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@aws-sdk/client-textract": "^3.54.0",
    "@azure/ai-form-recognizer": "4.0.0-beta.2",
    "@azure/identity": "^2.0.4",
    "@expo-google-fonts/dancing-script": "^0.2.2",
    "@expo-google-fonts/josefin-sans": "^0.2.2",
    "@expo-google-fonts/montserrat": "^0.2.2",
    "@expo-google-fonts/roboto": "^0.2.2",
    "@expo-google-fonts/work-sans": "^0.2.2",
    "@expo/vector-icons": "^12.0.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@react-navigation/stack": "^6.0.11",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "expo": "^44.0.0",
    "expo-asset": "~8.4.6",
    "expo-camera": "~12.1.2",
    "expo-constants": "~13.0.1",
    "expo-file-system": "~13.1.4",
    "expo-font": "~10.0.4",
    "expo-linking": "~3.0.0",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.6",
    "expo-web-browser": "~10.1.0",
    "firebase": "^9.6.10",
    "lottie-react-native": "5.0.1",
    "organize-imports-cli": "^0.9.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^7.30.0",
    "react-native": "0.64.3",
    "react-native-animated-loader": "^0.0.9",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-paper": "^4.11.2",
    "react-native-particle-background": "^1.0.4",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-skeleton-content": "^1.0.24",
    "react-native-step-indicator": "^1.0.3",
    "react-native-svg": "12.1.1",
    "react-native-vector-icons": "^9.1.0",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.4.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "^0.67.2",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.29.2",
    "eslint-plugin-react-hooks": "4.3.0",
    "expo-cli": "^5.2.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "jest-expo": "^44.0.0",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "react-test-renderer": "17.0.2",
    "typescript": "~4.3.5"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": [
      "organize-imports-cli",
      "prettier --write",
      "eslint --fix"
    ]
  }
